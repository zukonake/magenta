#wont get implicitly called
sampleTable =
{
	abc = 2;
	blahBlah = "asdasdasdfg";
	io.print( blahBlah );
}

#explicit parameters
#implicit return type
sum( Integer a, Integer b ) =
{
	a + b;
}

